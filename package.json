{
  "name": "ts-node-example",
  "version": "0.0.1",
  "description": "ts-node example with ts-jest.",
  "main": "index.ts",
  "author": "dooboolab",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node node_modules/ts-node/dist/bin.js ./src/index.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint **/*.ts",
    "test": "jest -w 1",
    "test:coverage": "jest -w 1 --coverage",
    "test:watch": "jest --watch",
    "codecov": "codecov"
  },
  "dependencies": {
    "jest": "^26.0.1",
    "koa": "^2.11.0",
    "koa-jwt": "^3.6.0",
    "koa-router": "^8.0.8",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1"
  },
  "devDependencies": {
    "@koa/cors": "^3.0.0",
    "@types/jest": "^25.2.1",
    "@types/koa": "^2.11.3",
    "@types/koa-router": "^7.4.0",
    "@types/koa__cors": "^3.0.1",
    "@types/node": "^14.0.1",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "codecov": "^3.6.5",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-typescript": "^0.14.0",
    "install": "^0.13.0",
    "nodemon": "^2.0.3",
    "npm": "^6.14.6",
    "standard": "^14.3.4",
    "supertest": "^4.0.2",
    "typescript": "^3.9.2",
    "typescript-eslint-parser": "^22.0.0"
  }
}
